<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Appointment.
    -->
    <changeSet id="20170717150033-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="customer_profile_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment_customer_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer_profile"/>

        <addForeignKeyConstraint baseColumnNames="assigned_agent_profile_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment_assigned_agent_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agent_profile"/>

    </changeSet>
</databaseChangeLog>
